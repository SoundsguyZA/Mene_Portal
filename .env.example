# MENE PORTAL - Environment Configuration

# Server Configuration
PORT=3001
NODE_ENV=development

# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
KINDROID_API_KEY=your_kindroid_api_key_here
KINDROID_AI_ID=your_kindroid_ai_id_here

# Optional Services
COGNIVAULT_URL=http://localhost:8501
SOLLY_URL=http://localhost:5555

# Memory Settings
TRUTH_PROTOCOL=true
MEMORY_RETENTION_DAYS=90

# Feature Flags
ENABLE_BROWSER_AUTOMATION=true
ENABLE_COGNIVAULT=true
ENABLE_SHARED_MEMORY=true
