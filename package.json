{
  "name": "meneportal",
  "version": "1.0.0",
  "description": "Multi-agent chat portal application with AI integrations",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop mene-portal",
    "pm2:restart": "pm2 restart mene-portal",
    "pm2:logs": "pm2 logs mene-portal --nostream",
    "pm2:status": "pm2 status",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "deploy": "npm run docker:build && npm run docker:up",
    "install:python": "pip install -r requirements.python.txt",
    "setup": "npm install && npm run install:python"
  },
  "keywords": [
    "ai",
    "chat",
    "multi-agent",
    "openai",
    "kindroid",
    "groq",
    "speech-to-text"
  ],
  "author": "MenePortal Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "pm2": "^6.0.8"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "homepage": "http://."
}
